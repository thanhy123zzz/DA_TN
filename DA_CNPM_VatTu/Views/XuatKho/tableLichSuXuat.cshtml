@{
    string dayToString(DateTime? a)
    {
        if (a == null)
        {
            return "";
        }
        return a.Value.ToString("dd-MM-yyyy");
    }
}
    @foreach (PhieuXuatKho p in ViewBag.ListPhieuXuat)
    {
          <tr>
              <td>@p.SoPx</td>
              <td>@p.IdkhNavigation.TenKh</td>
              <td class="text-center">@p.NgayTao.Value.ToString("dd-MM-yyyy HH:mm")</td>
              <td>@p.SoHd</td>
              <td class="text-center">@dayToString(p.NgayHd)</td>
              <td>@p.IdnvNavigation.TenNv</td>
              <td>@p.GhiChu</td>
              <td class="last-td-column">
                  <div class="action justify-content-center">
                          <button type="button" value="@p.Id" class="text-primary btn-view-pn">
                              <i class="lni lni-eye"></i>
                          </button>
                          <a href="/QuanLy/XuatKho/download/phieuxuatkho/@p.Id" class="text-success">
                            <i class="lni lni-printer"></i>    
                      </a>
                  </div>
              </td>
          </tr>
    }